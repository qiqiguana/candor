<?xml version="1.0" encoding="UTF-8"?>
<types>
    <type id="ST">
        <description>
            string data
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="DT">
        <description>
            date ( Format: YYYY[MM[DD]])
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="NM">
        <description>
            numeric
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.NumericDataTypeReader"/>
        </parts>
    </type>
    <type id="ID">
        <description>
            coded value for HL7-defined tables
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="IS">
        <description>
           coded value for userdefined tables
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="FT">
        <description>
          formatted text data
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="SI">
        <description>
          sequence ID
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="TS">
        <description>
            time stamp - Format: YYYY[MM[DD[HHMM[SS[.S[S[S[S]]]]]]]][+/-ZZZZ]^degree of precision
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="TX">
        <description>
            text data
        </description>
        <parts>
            <part reader="com.browsersoft.openhre.hl7.impl.parser.StringDataTypeReader"/>
        </parts>
    </type>
    <type id="HD">
        <description>
          hierarchic designator
        </description>
        <parts>
            <part subtype="IS" description="namespace ID"/>
            <part subtype="ST" description="universal ID"/>
            <part subtype="ID" description="universal ID type"/>
        </parts>
    </type>
    <type id="MO">
        <description>
          money
        </description>
        <parts>
            <part subtype="NM" description="quantity"/>
            <part subtype="ID" description="denomination"/>
        </parts>
    </type>
    <type id="CE">
        <description>
          coded element with formatted values
        </description>
        <parts>
            <part subtype="ST" description="identifier"/>
            <part subtype="ST" description="text" />
            <part subtype="ST" description="name of coding system"/>
            <part subtype="ST" description="alternate identifier"/>
            <part subtype="ST" description="alternate text"/>
            <part subtype="ST" description="name of alternate coding system"/>
        </parts>
    </type>
    <type id="CK">
        <description>
          composite ID with check digit
        </description>
        <parts>
            <part subtype="NM" description="ID number"/>
            <part subtype="NM" description="check digit"/>
            <part subtype="ID" description="code identifying the check digit scheme employed"/>
            <part subtype="HD" description="assigning authority"/>
        </parts>
    </type>
    <type id="CM" contentType="combination">
        <description>
          composite
        </description>
        <parts>
        </parts>
    </type>
    <type id="CP">
        <description>
          composite price
        </description>
        <parts>
            <part subtype="MO" description="price"/>
            <part subtype="ID" description="price type"/>
            <part subtype="NM" description="from value"/>
            <part subtype="NM" description="to value"/>
            <part subtype="CE" description="range units"/>
            <part subtype="ID" description="range type"/>
        </parts>
    </type>
    <type id="CQ">
        <description>
          composite quantity with units
        </description>
        <parts>
            <part subtype="NM" description="quantity"/>
            <part subtype="CE" description="units"/>
        </parts>
    </type>
    <type id="CX">
        <description>
          extended composite ID with check digit
        </description>
        <parts>
            <part subtype="ST" table="0203" description="ID"/>
            <part subtype="ST" description="check digit"/>
            <part subtype="ID" description="code identifying the check digit scheme employed"/>
            <part subtype="HD" description="assigning authority"/>
            <part subtype="IS" description="identifier type code"/>
            <part subtype="HD" description="assigning facility"/>
        </parts>
    </type>
    <type id="DLN">
        <description>
          drivers license number
        </description>
        <parts>
            <part subtype="ST" description="license number"/>
            <part subtype="IS" description="issuing state, province, country"/>
            <part subtype="DT" description="expiration date"/>
        </parts>
    </type>
    <type id="EI">
        <description>
          entity identifier
        </description>
        <parts>
            <part subtype="ST" description="entity identifier"/>
            <part subtype="IS" description="namespace ID"/>
            <part subtype="ST" description="universal ID"/>
            <part subtype="ID" description="universal ID type"/>
        </parts>
    </type>
    <type id="FC">
        <description>
          financial class
        </description>
        <parts>
            <part subtype="IS" description="financial class"/>
            <part subtype="ST" description="effective date"/>
        </parts>
    </type>
    <type id="JCC">
        <description>
          job code/class
        </description>
        <parts>
            <part subtype="IS" description="job code"/>
            <part subtype="IS" description="job class"/>
        </parts>
    </type>
    <type id="PL">
        <description>
          person location
        </description>
        <parts>
            <part subtype="IS" description="point of care"/>
            <part subtype="IS" description="room"/>
            <part subtype="IS" description="bed"/>
            <part subtype="HD" description="facility"/>
            <part subtype="IS" description="location status"/>
            <part subtype="IS" description="person location type"/>
            <part subtype="IS" description="building"/>
            <part subtype="IS" description="floor"/>
            <part subtype="ST" description="location description"/>
        </parts>
    </type>
    <type id="PN">
        <description>
          person name
        </description>
        <parts>
            <part subtype="ST" description="family name"/>
            <part subtype="ST" description="last name prefix"/>
            <part subtype="ST" description="given name"/>
            <part subtype="ST" description="middle initial or name"/>
            <part subtype="ST" description="suffix (e.g., Jr. or III)"/>
            <part subtype="ST" description="prefix (e.g., Dr.)"/>
            <part subtype="IS" description="degree (e.g., MD)" table="0360"/>
        </parts>
    </type>
    <type id="PT">
        <description>
          processing type
        </description>
        <parts>
            <part subtype="ID" description="processing ID"/>
            <part subtype="ID" description="processing mode"/>
        </parts>
    </type>
    <type id="TQ">
        <description>
          timing quantity
        </description>
        <parts>
            <part subtype="CQ" description="quantity"/>
            <part subtype="CM" description="interval"/>
            <part subtype="ST" description="duration"/>
            <part subtype="TS" description="start date/time"/>
            <part subtype="TS" description="end date/time"/>
            <part subtype="ST" description="priority"/>
            <part subtype="ST" description="condition"/>
            <part subtype="TX" description="text"/>
            <part subtype="ST" description="conjunction"/>
            <part subtype="CM" description="order sequencing"/>
            <part subtype="CE" description="performance duration"/>
            <part subtype="NM" description="total occurrences"/>
        </parts>
    </type>
    <type id="VID">
        <description>
          version identifier
        </description>
        <parts>
            <part subtype="ID" description="version ID" table="0104"/>
            <part subtype="CE" description="internationalization code" table="0212"/>
            <part subtype="CE" description="international version ID"/>
        </parts>
    </type>
    <type id="XAD">
        <description>
          extended address
        </description>
        <parts>
            <part subtype="ST" description="street address"/>
            <part subtype="ST" description="other designation"/>
            <part subtype="ST" description="city"/>
            <part subtype="ST" description="state or province"/>
            <part subtype="ST" description="zip or postal code"/>
            <part subtype="ID" description="country" table="0212"/>
            <part subtype="ID" description="address type" table="0190"/>
            <part subtype="ST" description="other geographic designation"/>
            <part subtype="IS" description="county/parish code" table="0289"/>
            <part subtype="IS" description="census tract" table="0288"/>
            <part subtype="ID" description="address representation code" table="4000"/>
        </parts>
    </type>
    <type id="XCN">
        <description>
          extended composite ID number and name for persons
        </description>
        <parts>
            <part subtype="ST" description="ID number"/>
            <part subtype="ST" description="family name"/>
            <part subtype="ST" description="last name prefix"/>
            <part subtype="ST" description="given name"/>
            <part subtype="ST" description="middle initial or name"/>
            <part subtype="ST" description="suffix (e.g., Jr. or III)"/>
            <part subtype="ST" description="prefix (e.g., Dr.)"/>
            <part subtype="IS" description="degree (e.g., MD)" table="0360"/>
            <part subtype="IS" description="source table" table="0297"/>
            <part subtype="HD" description="assigning authority"/>
            <part subtype="ID" description="name type code" table="0200"/>
            <part subtype="ST" description="identifier check digit"/>
            <part subtype="ID" description="code identifying the check digit scheme employed"/>
            <part subtype="IS" description="identifier type code" table="0203"/>
            <part subtype="HD" description="assigning facility ID"/>
            <part subtype="ID" description="name representation code" table="4000"/>
        </parts>
    </type>
    <type id="XON">
        <description>
          extended composite name and identification number for organizations
        </description>
        <parts>
            <part subtype="ST" description="organization name"/>
            <part subtype="IS" description="organization name type code" table="0204"/>
            <part subtype="NM" description="ID number"/>
            <part subtype="NM" description="check digit"/>
            <part subtype="ID" description="code identifying the check digit scheme employed"/>
            <part subtype="HD" description="assigning authority"/>
            <part subtype="IS" description="identifier type code" table="0203"/>
            <part subtype="HD" description="assigning facility ID"/>
            <part subtype="ID" description="name representation code" table="4000"/>
        </parts>
    </type>
    <type id="XPN">
        <description>
          extended person name
        </description>
        <parts>
            <part subtype="ST" description="family name"/>
            <part subtype="ST" description="last name prefix"/>
            <part subtype="ST" description="given name"/>
            <part subtype="ST" description="middle initial or name"/>
            <part subtype="ST" description="suffix (e.g., Jr. or III)"/>
            <part subtype="ST" description="prefix (e.g., Dr.)"/>
            <part subtype="IS" description="degree (e.g., MD)" table="0360"/>
            <part subtype="ID" description="name type code" table="0200"/>
            <part subtype="ID" description="name representation code" table="4000"/>
        </parts>
    </type>
    <type id="XTN">
        <description>
          extended telecommunication number
        </description>
        <parts>
            <part subtype="ST" description="unknown!!!!"/>
            <part subtype="ID" description="telecommunication use code" table="0201"/>
            <part subtype="ID" description="telecommunication equipment type" table="0202"/>
            <part subtype="ST" description="email address"/>
            <part subtype="NM" description="country code"/>
            <part subtype="NM" description="area/city code"/>
            <part subtype="NM" description="phone number"/>
            <part subtype="NM" description="extension"/>
            <part subtype="ST" description="any text"/>
        </parts>
    </type>
</types>
