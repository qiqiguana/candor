    <segment id="Z01">
        <description>
            Lab Product Segment
        </description>
        <fields>
            <field sequential="1" maximumLength="4" dataType="SI" required="O" reference="">
                <description>
                    Set ID
                </description>
            </field>
            <field sequential="2" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Product Type ID - This corresponds to the ID returned in the OBR.0403 HL7 Lab Result field.
                    The code that the lab source uses to identify the product.
                    This is the code that should be sent to the lab source to identify a product that is being ordered.
                </description>
            </field>
            <field sequential="3" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Name - The name of the product
                </description>
            </field>
            <field sequential="4" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Description - The description of the product.
                </description>
            </field>
            <field sequential="5" maximumLength="20" dataType="ST" required="O" reference="">
                <description>
                    CPT Code - The equivalent CPT code (if any).
                </description>
            </field>
            <field sequential="6" maximumLength="1" dataType="ST" required="O" reference="">
                <description>
                   Can Order Alone - Whether or not this product can be ordered alone (Y = Yes, N = No).
                   Some products, for example, can only be ordered as part of a group of products (i.e. panel or profile).
                </description>
            </field>
            <field sequential="7" maximumLength="1" dataType="ST" required="O" reference="">
                <description>
                    Is Panel - Whether or not this product is a panel (Y = Yes, N = No).
                </description>
            </field>
        </fields>
    </segment>
    <segment id="Z02">
        <description>
            Lab Sub-Product Segment
        </description>
        <fields>
            <field sequential="1" maximumLength="4" dataType="SI" required="R" reference="">
                <description>
                    Set ID - Z02
                </description>
            </field>
            <field sequential="2" maximumLength="50" dataType="ST" required="R" reference="">
                <description>
                    Main Product Type ID - The ID of the main product type
                </description>
            </field>
            <field sequential="3" maximumLength="50" dataType="ST" required="R" reference="">
                <description>
                    Sub Product Type ID - The ID of the product type that is ordered as part of the main product type
                </description>
            </field>
        </fields>
    </segment>
    <segment id="Z03">
        <description>
            Lab Test Segment
        </description>
        <fields>
            <field sequential="1" maximumLength="4" dataType="SI" required="O" reference="">
                <description>
                    Set ID - Z03
                </description>
            </field>
            <field sequential="2" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Test Type ID - The code that is used by the lab source to identify the test.
                    This is the code that is returned by the lab source to identify the test when a result
                    is being reported. This corresponds to the ID returned in the OBX.0303 field in the HL7 Lab Result.
                </description>
            </field>
            <field sequential="3" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Name - The name of the test
                </description>
            </field>
            <field sequential="4" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Description - The description of the test.
                </description>
            </field>
            <field sequential="5" maximumLength="1" dataType="ST" required="O" reference="">
                <description>
                    ValueType - The value type of the tests result (N = Number, T = Text, L = List).
                </description>
            </field>
            <field sequential="6" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Units - The units of measure of the tests result (i.e. mg/dL).
                </description>
            </field>
        </fields>
    </segment>
    <segment id="Z04">
        <description>
            Lab Product Test Segment
        </description>
        <fields>
            <field sequential="1" maximumLength="4" dataType="SI" required="O" reference="">
                <description>
                    Set ID - Z04
                </description>
            </field>
            <field sequential="2" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Product ID
                </description>
            </field>
            <field sequential="3" maximumLength="50" dataType="ST" required="O" reference="">
                <description>
                    Test Type ID - The ID of the test type that is performed when the product is ordered
                </description>
            </field>
        </fields>
    </segment>
</segments>